                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{homeVM.homeModel.txtSixteen}"
                                style="@style/txtPoppinsregular12_2" tools:text="@string/lbl15"/>
                        </LinearLayout>
                        <LinearLayout
