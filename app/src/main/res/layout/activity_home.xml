                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_48pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_weight="0.24"
                            android:layout_width="0dp" android:orientation="vertical">
