<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="massageVM" type="com.egoistenoksapplication.app.modules.massage.data.viewmodel.MassageVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearMassage"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_43pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearHeader"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowdown"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowdown"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMassage"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_110pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_weight="0.77"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{massageVM.massageModel.txtMassage}"
                    style="@style/txtPoppinsmedium20" tools:text="@string/lbl_massage"/>
                <ImageView android:id="@+id/imageSearch"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_3pxh"
                    android:layout_marginStart="@dimen/_100pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <androidx.recyclerview.widget.RecyclerView
            android:gravity="center_horizontal"
            android:id="@+id/recyclerListavatar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            itemSpace="@{@dimen/_30pxh}" tools:itemCount="3" tools:listitem="@layout/row_listavatar"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowavatarThree"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageAvatarThree"
                android:layout_height="@dimen/_40pxh"
                android:layout_width="@dimen/_40pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_avatar_2}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_avatar_2"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnrozannebarrienThree"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_weight="0.86" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtRozanneBarrienThree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{massageVM.massageModel.txtRozanneBarrienThree}"
                    style="@style/txtPoppinsmedium16_6" tools:text="@string/lbl_tony_stark"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{massageVM.massageModel.txtLanguage}"
                    style="@style/txtPoppinsregular14_1" tools:text="@string/msg22"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowb"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearRowavatarFour"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_weight="0.78" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageAvatarFour"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_avatar_40x20"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_avatar_40x20"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnrozannebarrienFour"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.89"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRozanneBarrienFour"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{massageVM.massageModel.txtRozanneBarrienFour}"
                        style="@style/txtPoppinsmedium16_6" tools:text="@string/lbl_banner"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtThenewLeanstThree"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{massageVM.massageModel.txtThenewLeanstThree}"
                        style="@style/txtPoppinsregular14_1" tools:text="@string/msg23"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearDelete"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_125pxh"
                android:layout_weight="0.22" android:layout_width="0dp"
                android:orientation="horizontal" style="@style/groupStylered_500">
                <TextView android:ellipsize="end" android:id="@+id/txtB"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_weight="-0.04"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{massageVM.massageModel.txtB}"
                    style="@style/txtPoppinsmedium16_7" tools:text="@string/lbl_b"/>
                <ImageView android:id="@+id/imageTrash"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_333pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_17pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_trash"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_trash"/>
            </LinearLayout>
        </LinearLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:gravity="center_horizontal"
            android:id="@+id/recyclerListavatarFive"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            itemSpace="@{@dimen/_30pxh}" tools:itemCount="2" tools:listitem="@layout/row_listavatar_five"/>
        <androidx.recyclerview.widget.RecyclerView
            android:gravity="center_horizontal"
            android:id="@+id/recyclerListgrouptwentynine"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            itemSpace="@{@dimen/_30pxh}" tools:itemCount="2" tools:listitem="@layout/row_listgrouptwentynine"/>
        <FrameLayout android:id="@+id/frameNavigation"
            android:layout_height="@dimen/_92pxv"
            android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
            <View
                android:background="@drawable/rectangle_gradient_s_gray_900_11_e_gray_400_11"
                android:id="@+id/viewShadow"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_48pxv"
                android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_375pxh"/>
            <LinearLayout android:id="@+id/linearRoweye"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_3">
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearColumnhome"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_37pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_weight="0.55"
                    android:layout_width="0dp" android:orientation="vertical">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowhome"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_49pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearHome"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <ImageView android:id="@+id/imageHome"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginEnd="@dimen/_14pxh"
                                android:layout_marginStart="@dimen/_14pxh"
                                android:layout_width="@dimen/_19pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_home_20x19"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_home_20x19"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtThirtySeven"
                                android:layout_gravity="start"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{massageVM.massageModel.txtThirtySeven}"
                                style="@style/txtPoppinsregular12_2" tools:text="@string/lbl38"/>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageChat"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_17pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_chat"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_chat"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageEye"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginEnd="@dimen/_51pxh"
                        android:layout_marginStart="@dimen/_51pxh"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_eye"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_eye"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearProfile"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_weight="0.25"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginEnd="@dimen/_19pxh"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_20x16"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_20x16"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtThirtyEight"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{massageVM.massageModel.txtThirtyEight}"
                        style="@style/txtPoppinsregular12_2" tools:text="@string/lbl15"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearSetting"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_weight="0.2"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageSettings"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_settings_20x19"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_settings_20x19"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtThirtyNine"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{massageVM.massageModel.txtThirtyNine}"
                        style="@style/txtPoppinsregular12_2" tools:text="@string/lbl16"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
