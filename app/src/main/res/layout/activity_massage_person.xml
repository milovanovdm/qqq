<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="massagePersonVM" type="com.egoistenoksapplication.app.modules.massageperson.data.viewmodel.MassagePersonVM"/>
    </data>
    <FrameLayout android:id="@+id/frameMassagePerson"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_50">
        <LinearLayout android:id="@+id/linearColumnarrowdown"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_40pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearHeader"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowdown"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowdown"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown"/>
                    <LinearLayout android:id="@+id/linearRowavatar"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_93pxh"
                        android:layout_weight="0.96"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageAvatar"
                            android:layout_height="@dimen/_27pxh"
                            android:layout_width="@dimen/_27pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_avatar_27x27}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_avatar_27x27"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtRozanneBarrien"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_13pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.67"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{massagePersonVM.massagePersonModel.txtRozanneBarrien}"
                            style="@style/txtPoppinsmedium16_6" tools:text="@string/lbl_anaya_sanji"/>
                        <ImageView android:id="@+id/imageCall"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_74pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_call"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_call"/>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearMessage"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearMymessage"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recyclerMassagePerson"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        itemSpace="@{@dimen/_16pxh}" tools:itemCount="2" tools:listitem="@layout/row_massage_person"/>
                    <FrameLayout
                        android:id="@+id/frameStackrectanglecopyTwo"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_56pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_181pxh" style="@style/groupStylecornerRadius_2">
                        <ImageView
                            android:id="@+id/imageRectangleCopyTwo"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_56pxh"
                            android:layout_width="@dimen/_181pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_rectanglecopy"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectanglecopy"/>
                        <LinearLayout
                            android:id="@+id/linearColumnhowareyou"
                            android:layout_gravity="top|center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtHowareyou"
                                android:layout_gravity="start"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{massagePersonVM.massagePersonModel.txtHowareyou}"
                                style="@style/txtPoppinsmedium14_1" tools:text="@string/lbl40"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTimeTwo"
                                android:layout_gravity="end"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_9pxv"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{massagePersonVM.massagePersonModel.txtTimeTwo}"
                                style="@style/txtPoppinsregular12_3" tools:text="@string/lbl_4_35_pm"/>
                        </LinearLayout>
                    </FrameLayout>
                    <FrameLayout
                        android:id="@+id/frameStackrectanglecopyThree"
                        android:layout_height="@dimen/_55pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_233pxh" style="@style/groupStylecornerRadius_2">
                        <ImageView
                            android:id="@+id/imageRectangleCopyThree"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_55pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_rectanglecopy_white_a701"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectanglecopy_white_a701"/>
                        <LinearLayout
                            android:id="@+id/linearColumnnicebro"
                            android:layout_gravity="top|center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtNicebro"
                                android:layout_gravity="start"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{massagePersonVM.massagePersonModel.txtNicebro}"
                                style="@style/txtPoppinsmedium14_1" tools:text="@string/lbl41"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTimeThree"
                                android:layout_gravity="end"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_8pxv"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{massagePersonVM.massagePersonModel.txtTimeThree}"
                                style="@style/txtPoppinsregular12_3" tools:text="@string/lbl_4_34_pm"/>
                        </LinearLayout>
                    </FrameLayout>
                    <FrameLayout
                        android:id="@+id/frameStackrectanglecopyFour"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_77pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_238pxh" style="@style/groupStylecornerRadius_2">
                        <ImageView
                            android:id="@+id/imageRectangleCopyFour"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_77pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_rectanglecopy"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectanglecopy"/>
                        <LinearLayout android:id="@+id/linearColumnokay"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <TextView android:gravity="start"
                                android:id="@+id/txtOkay"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_6pxh"
                                android:text="@{massagePersonVM.massagePersonModel.txtOkay}"
                                style="@style/txtPoppinsmedium14_1" tools:text="@string/msg24"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTimeFour"
                                android:layout_gravity="end"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_6pxv"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{massagePersonVM.massagePersonModel.txtTimeFour}"
                                style="@style/txtPoppinsregular12_3" tools:text="@string/lbl_4_35_pm"/>
                        </LinearLayout>
                    </FrameLayout>
                    <FrameLayout
                        android:id="@+id/frameStackrectanglecopyFive"
                        android:layout_height="@dimen/_76pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_212pxh" style="@style/groupStylecornerRadius_2">
                        <ImageView
                            android:id="@+id/imageRectangleCopyFive"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_76pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_rectanglecopy_white_a701"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectanglecopy_white_a701"/>
                        <LinearLayout
                            android:id="@+id/linearColumndidyouseenew"
                            android:layout_gravity="end"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtDidyouseenew"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{massagePersonVM.massagePersonModel.txtDidyouseenew}"
                                style="@style/txtPoppinsmedium14_1" tools:text="@string/msg25"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTimeFive"
                                android:layout_gravity="end"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_18pxh"
                                android:layout_marginStart="@dimen/_18pxh"
                                android:layout_marginTop="@dimen/_25pxv"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{massagePersonVM.massagePersonModel.txtTimeFive}"
                                style="@style/txtPoppinsregular12_3" tools:text="@string/lbl_4_34_pm"/>
                        </LinearLayout>
                    </FrameLayout>
                    <FrameLayout
                        android:id="@+id/frameStackrectanglecopySix"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_63pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_182pxh" style="@style/groupStylecornerRadius_2">
                        <ImageView
                            android:id="@+id/imageRectangleCopySix"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_63pxh"
                            android:layout_width="@dimen/_182pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_rectanglecopy"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectanglecopy"/>
                        <LinearLayout
                            android:id="@+id/linearColumnokayOne"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtOkayOne"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{massagePersonVM.massagePersonModel.txtOkayOne}"
                                style="@style/txtPoppinsmedium14_1" tools:text="@string/lbl42"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTimeSix"
                                android:layout_gravity="end"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_12pxv"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{massagePersonVM.massagePersonModel.txtTimeSix}"
                                style="@style/txtPoppinsregular12_3" tools:text="@string/lbl_4_34_pm"/>
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowplus"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_78pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_teal_300_radius_23"
                    android:id="@+id/btnPlus"
                    android:layout_height="@dimen/_46pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_46pxh"
                    android:paddingHorizontal="@dimen/_15pxh"
                    android:paddingVertical="@dimen/_15pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_plus" tools:ignore="ContentDescription"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_radius_10"
                    android:drawableEnd="@drawable/img_send"
                    android:drawablePadding="@dimen/_30pxh"
                    android:ellipsize="end"
                    android:hint="@string/lbl_type_a_message"
                    android:id="@+id/etMassage" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_weight="0.86"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_20pxh"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_22pxv"
                    android:singleLine="true"
                    android:text="@={massagePersonVM.massagePersonModel.etMassageValue}"
                    android:textColorHint="@color/gray_506"
                    style="@style/etSolidRounded_3" tools:ignore="Autofill"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
