<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpVM" type="com.egoistenoksapplication.app.modules.signup.data.viewmodel.SignUpVM"/>
    </data>
    <LinearLayout android:id="@+id/linearSignUp"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <ImageView android:id="@+id/imageArrowleft"
            android:layout_gravity="start"
            android:layout_height="@dimen/_21pxh"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="@dimen/_10pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_arrowleft"
            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtEight"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_42pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{signUpVM.signUpModel.txtEight}"
            style="@style/txtPoppinssemibold30" tools:text="@string/msg6"/>
        <TextView android:ellipsize="end" android:gravity="start"
            android:id="@+id/txtLanguage"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{signUpVM.signUpModel.txtLanguage}"
            style="@style/txtPoppinsregular16" tools:text="@string/msg3"/>
        <EditText
            android:background="@drawable/rectangle_bg_white_a700_radius_12"
            android:drawablePadding="@dimen/_15pxh"
            android:drawableStart="@drawable/img_user"
            android:ellipsize="end" android:hint="@string/lbl_user_name"
            android:id="@+id/etName" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_56pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_18pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_20pxh"
            android:paddingTop="@dimen/_19pxv" android:singleLine="true"
            android:text="@={signUpVM.signUpModel.etNameValue}"
            android:textColorHint="@color/gray_700"
            style="@style/etSolidRounded" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_bg_white_a700_radius_12"
            android:drawablePadding="@dimen/_17pxh"
            android:drawableStart="@drawable/img_message"
            android:ellipsize="end"
            android:hint="@string/lbl_email_address"
            android:id="@+id/etEmail"
            android:inputType="textEmailAddress"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_19pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_21pxh"
            android:paddingTop="@dimen/_18pxv" android:singleLine="true"
            android:text="@={signUpVM.signUpModel.etEmailValue}"
            android:textColorHint="@color/gray_700"
            style="@style/etSolidRounded" tools:ignore="Autofill"/>
        <LinearLayout android:id="@+id/linearPassword"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadius">
            <LinearLayout android:id="@+id/linearRowlock"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_weight="0.87" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageLock"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lock_26x20"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_lock_26x20"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_weight="0.79"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{signUpVM.signUpModel.txtPassword}"
                    style="@style/txtPoppinsregular16" tools:text="@string/lbl_password"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageAlarm"
                android:layout_height="@dimen/_13pxh"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_166pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="@dimen/_16pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_alarm"
                tools:ignore="ContentDescription" tools:src="@drawable/img_alarm"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:paddingBottom="@dimen/_18pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_18pxv" android:singleLine="true"
            android:text="@string/lbl7" android:textAllCaps="false"
            style="@style/btnSolidRounded" tools:text="@string/lbl7"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnline210"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="wrap_content" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowline210"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <View android:background="@color/gray_500"
                    android:id="@+id/lineLine210"
                    android:layout_height="@dimen/_2pxv"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_weight="0.12" android:layout_width="0dp"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguageOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_weight="0.77"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{signUpVM.signUpModel.txtLanguageOne}"
                    style="@style/txtPoppinsregular16" tools:text="@string/lbl8"/>
                <View android:background="@color/gray_500"
                    android:id="@+id/lineLine211"
                    android:layout_height="@dimen/_2pxv"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_weight="0.12" android:layout_width="0dp"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageSocial"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_60pxh"
                android:layout_marginEnd="@dimen/_27pxh"
                android:layout_marginStart="@dimen/_27pxh"
                android:layout_marginTop="@dimen/_28pxv"
                android:layout_width="@dimen/_140pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_social"
                tools:ignore="ContentDescription" tools:src="@drawable/img_social"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtNine"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_46pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{signUpVM.signUpModel.txtNine}"
            style="@style/txtSofiaProregular16" tools:text="@string/msg7"/>
    </LinearLayout>
</layout>
