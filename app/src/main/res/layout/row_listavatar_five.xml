<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listavatarFiveRowModel" type="com.egoistenoksapplication.app.modules.massage.data.model.ListavatarFiveRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowavatar"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageAvatarFive"
            android:layout_height="@dimen/_40pxh"
            android:layout_width="@dimen/_40pxh"
            android:scaleType="centerCrop" circular="@{true}"
            imageRes="@{@drawable/img_avatar_3}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_avatar_3"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnrozannebarrienFive"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_weight="0.88" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtRozanneBarrienFive"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{listavatarFiveRowModel.txtRozanneBarrienFive}"
                style="@style/txtPoppinsmedium16_6" tools:text="@string/lbl_steave"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtThenewLeanstFour"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{listavatarFiveRowModel.txtThenewLeanstFour}"
                style="@style/txtPoppinsregular14_1" tools:text="@string/msg_move_in_some_sp"/>
        </LinearLayout>
    </LinearLayout>
</layout>
